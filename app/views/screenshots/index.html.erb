<!--< % title "Screenshots" % >-->

<table>
  <tr>
    <th>Application</th>
    <th>Name</th>
    <th>Shot File Name</th>
    <th>Shot Content Type</th>
    <th>Shot File Size</th>
    <th>Shot Updated At</th>
    <th>View</th>
  </tr>
  <% for screenshot in @screenshots %>
    <tr>
      <td><%=h screenshot.application %></td>
      <td><%=h screenshot.name %></td>
      <td><%=h screenshot.shot_file_name %></td>
      <td><%=h screenshot.shot_content_type %></td>
      <td><%=h screenshot.shot_file_size %></td>
      <td><%=h screenshot.shot_updated_at %></td>
      <td><%=h screenshot.view %></td>
      <td><%= link_to "Show", screenshot %></td>
      <td><%= link_to "Edit", edit_screenshot_path(screenshot) %></td>
      <td><%= link_to "Destroy", screenshot, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Screenshot", new_screenshot_path %></p>
