<h1>New site_configuration</h1>

<% form_for(@site_configuration) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :website %><br />
    <%= f.text_field :website %>
  </p>
  <p>
    <%= f.label :header %><br />
    <%= f.text_field :header %>
  </p>
  <p>
    <%= f.label :webiso %><br />
    <%= f.text_field :webiso %>
  </p>
  <p>
    <%= f.label :root_category_id %><br />
    <%= f.text_field :root_category_id %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', site_configurations_path %>
